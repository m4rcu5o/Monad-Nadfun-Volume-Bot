# Monad Mainnet RPC URL
RPC_URL=https://rpc3.monad.xyz

# Chain ID for Monad Mainnet
CHAIN_ID=143

# Master wallet private key (for funding distribution)
MASTER_PRIVATE_KEY=your_master_wallet_private_key_here

# nadfun contract addresses
NADFUN_ROUTER_ADDRESS=0x0000000000000000000000000000000000000000
NADFUN_FACTORY_ADDRESS=0x0000000000000000000000000000000000000000

# Token address to create volume for
TOKEN_ADDRESS=

# Trading configuration
MIN_BUY_AMOUNT_MONAD=0.001
MAX_BUY_AMOUNT_MONAD=0.1
MIN_SELL_PERCENTAGE=50
MAX_SELL_PERCENTAGE=100

# Volume bot configuration
NUMBER_OF_WALLETS=10
DISTRIBUTION_AMOUNT_PER_WALLET=0.5
DELAY_BETWEEN_TXS_MS=5000
RANDOM_DELAY_MS=3000

# Gas configuration
MAX_GAS_PRICE_GWEI=50
GAS_LIMIT=500000

# Safety features
ENABLE_SLIPPAGE_PROTECTION=true
MAX_SLIPPAGE_PERCENT=5
STOP_LOSS_PERCENT=20
